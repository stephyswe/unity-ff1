{
    "files.exclude":
    {
        "**/.DS_Store":true,
        "**/.git":true,
        "**/.gitignore":true,
        "**/.gitmodules":true,
        "**/*.booproj":true,
        "**/*.pidb":true,
        "**/*.suo":true,
        "**/*.user":true,
        "**/*.userprefs":true,
        "**/*.unityproj":true,
        "**/*.dll":true,
        "**/*.exe":true,
        "**/*.pdf":true,
        "**/*.mid":true,
        "**/*.midi":true,
        "**/*.wav":true,
        "**/*.gif":true,
        "**/*.ico":true,
        "**/*.jpg":true,
        "**/*.jpeg":true,
        "**/*.png":true,
        "**/*.psd":true,
        "**/*.tga":true,
        "**/*.tif":true,
        "**/*.tiff":true,
        "**/*.3ds":true,
        "**/*.3DS":true,
        "**/*.fbx":true,
        "**/*.FBX":true,
        "**/*.lxo":true,
        "**/*.LXO":true,
        "**/*.ma":true,
        "**/*.MA":true,
        "**/*.obj":true,
        "**/*.OBJ":true,
        "**/*.asset":true,
        "**/*.cubemap":true,
        "**/*.flare":true,
        "**/*.mat":true,
        "**/*.meta":true,
        "**/*.prefab":true,
        "**/*.unity":true,
        "build/":true,
        "Build/":true,
        "Library/":true,
        "library/":true,
        "obj/":true,
        "Obj/":true,
        "ProjectSettings/":true,
        "temp/":true,
        "Temp/":true
    },
    "[csharp]": {
        "editor.rulers": [
            120
        ],
        "editor.tabSize": 4,
        "editor.insertSpaces": true,
        "editor.wordWrapColumn": 120,
        "editor.formatOnPaste": true,
        "editor.formatOnType": true,
        "editor.formatOnSave": true,
        "csharp.format.insertSpaceAfterMethodCallName": false,
        "csharp.format.placeOpenBraceOnNewLineForControlBlocks": false,
        "csharp.format.placeOpenBraceOnNewLineForFunctions": false,
        "csharp.format.placeOpenBraceOnNewLineForAnonymousMethods": false,
        "csharp.format.placeOpenBraceOnNewLineForMethods": false,
        "csharp.format.placeOpenBraceOnNewLineForTypes": false,
        "csharp.format.placeOpenBraceOnNewLineForObjectInitializers": false,
    }

}